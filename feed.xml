<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Erixpage.de</title>
    <description>Technik, Politik, Webdesign und Meinung</description>
    <link>http://www.erixpage.de/</link>
    <atom:link href="http://www.erixpage.de/feed.xml" rel="self" type="application/rss+xml" />
    {% for post in site.posts limit:10 %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <description>{{ post.content | xml_escape }}</description>
        <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <link>http://www.erixpage.de{{ post.url }}</link>
        <guid isPermaLink="true">http://www.erixpage.de{{ post.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>